{
  "name": "@tomtom-international/web-sdk-services",
  "version": "6.22.1",
  "description": "TomTom Maps SDK for Web - Services library",
  "main": "dist/services.min.js",
  "typings": "dist/index",
  "files": [
    "dist/services*",
    "dist/index*",
    "esm*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist esm.js* *.zip *.tgz",
    "copy:main-config": "node ../../scripts/copyMainConfig",
    "build:fast": "npm run copy:main-config && webpack --env production && rollup -c",
    "build:watch": "webpack -w & npm start",
    "build": "npm run clean && npm run lint && npm run build:fast && npm run package",
    "lint": "eslint -c ../../.eslintrc ../../src/ ../../test/",
    "package": "pack-to-zip",
    "prepack": "node ../../scripts/createSdkLicense services",
    "start": "http-server ./dist",
    "test": "npm run copy:main-config && karma start --singleRun=true --devtool=none",
    "test:dev": "npm run copy:main-config && karma start --devtool=inline-source-map",
    "test:integration": "npm run copy:main-config && karma start --integration --singleRun --log-level debug",
    "test:integration:dev": "npm run copy:main-config && karma start --integration --devtool=inline-source-map",
    "security:audit": "npm audit --audit-level=moderate --production"
  },
  "dependencies": {
    "@types/geojson": "7946.0.7",
    "lodash.castarray": "4.4.0",
    "lodash.flatten": "4.4.0",
    "lodash.has": "4.5.2",
    "lodash.isempty": "4.4.0",
    "lodash.isfunction": "3.0.9",
    "lodash.isinteger": "4.0.4",
    "lodash.isnil": "4.0.0",
    "lodash.isobject": "3.0.2",
    "lodash.isstring": "4.0.1",
    "lodash.isundefined": "3.0.1",
    "node-fetch": "3.2.10",
    "regenerator-runtime": "0.12.1",
    "uuid": "8.3.2"
  },
  "author": "TomTom",
  "homepage": "https://developer.tomtom.com/maps-sdk-web-js",
  "license": "SEE LICENSE IN LICENSE.txt",
  "keywords": [
    "tomtom",
    "maps",
    "routing",
    "geocoding",
    "search",
    "navigation",
    "traffic"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.tomtomgroup.com:7999/jssdk/elysium-sdk.git"
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-transform-async-to-generator": "7.13.0",
    "@babel/preset-env": "7.13.9",
    "@jsdevtools/coverage-istanbul-loader": "3.0.5",
    "@rollup/plugin-alias": "3.1.2",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "axios-mock-adapter": "1.14.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-lodash": "3.3.4",
    "copy-webpack-plugin": "7.0.0",
    "dotenv": "16.0.3",
    "eslint": "5.9.0",
    "http-server": "0.12.3",
    "jasmine-core": "3.6.0",
    "karma": "6.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0",
    "pack-to-zip": "1.0.0",
    "rollup": "2.39.1",
    "rollup-plugin-terser": "7.0.2",
    "webpack": "5.72.0",
    "webpack-cli": "4.5.0",
    "webpack-merge": "5.7.3"
  },
  "gitHead": "b5cdcc7d117828ab489f13e2edac3ae6eb379396"
}
